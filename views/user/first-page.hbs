<div style="background-color: black;">
    <div class="container-fluid">

        <div class="row ">
            <div class="col-md-3 " style="background-color: #343a40; text-align: center;">
                <a href="" class="btn btn-dark" style="width: 100%; color: #ffc107;">𝙋𝙀𝙏𝙎</a>
            </div>
            <div class="col-md-3 " style="background-color: #ffc107;text-align: center;">
                <a href="" class="btn btn-warning" style="width: 100%;">𝘼𝘾𝘾𝙀𝙎𝙎𝙊𝙍𝙄𝙀𝙎</a>
            </div>
            <div class="col-md-3 " style="background-color: #343a40; text-align: center;">
                <a href="" class="btn btn-dark" style="width: 100%; color: #ffc107;">𝙁𝙊𝙊𝘿</a>
            </div>
            <div class="col-md-3 " style="background-color: #ffc107; text-align: center;">
                <a href="" class="btn btn-warning" style="width: 100%;">𝙄𝙉𝙎𝙏𝙍𝙐𝘾𝙏𝙄𝙊𝙉𝙎</a>
            </div>
        </div>



        <div class="row">
            {{#each products}}

            <div class=" col-md-3 mt-5 p-5">
                <a class=" mt-5" href="/cart?id={{this._id}}" style="text-decoration: none; color: #fff;">
                    <div class="card m-auto"
                        style="width: 18rem;  border-radius: 20px; background-color: black; box-shadow: rgba(222, 201, 15, 0.3) 0px 0px 0px 3px; height: 500px; margin-top: 200em;">
                        <img class="card-img-top p-3" src="admin_image/{{this._id}}.jpg" alt="Card image cap"
                            style="border-radius: 30px; height: 200px;">
                        <div class="card-body" style="text-align: center;">
                            <h5 class="card-title" style="color: #ffc107;">{{this.name}}</h5>
                            <p class="card-text">{{this.discription}}</p>
                            <p>₹ {{this.price}}/-</p>

                        </div>
                    </div>
                </a>



            </div>
            {{/each}}

        </div>



    </div>
</div>